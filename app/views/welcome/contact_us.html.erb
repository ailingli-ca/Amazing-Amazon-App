<%= render "shared/navbar" %>
<h1>Contact us</h1>

<%= form_with url: "/thank_you", method: :get do |form| %>
    <%= form.label :name %>
    <%= form.text_field:name %>
    <%= form.label :email %>
    <%= form.text_field :email %>
    <%= form.label :text %>
    <%= form.text_field :text %>

    <%= form.submit "SUBmit" %>
<% end %>

